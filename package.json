{
  "name": "vue3-element-admin",
  "description": "Vite + Vue3 + TypeScript + Element-Plus + Unicss 中后台模板",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint \"src/**/*.{vue,ts,js}\" --fix",
    "lint:prettier": "prettier --write \"src/**/*.{js,cjs,ts,json,css,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint  \"src/**/*.{css,scss,less,vue,html}\"",
    "lint:stylelint:fix": "stylelint  \"src/**/*.{css,scss,less,vue,html}\" --fix",
    "lint:lint-staged": "lint-staged",
    "lint": "pnpm run lint:eslint && pnpm run lint:prettier && pnpm lint:stylelint:fix",
    "prepare": "husky",
    "commit": "git-cz",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "axios": "^1.13.2",
    "element-plus": "^2.12.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.24",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.1",
    "@iconify-json/ep": "^1.2.3",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.2",
    "fast-glob": "^3.3.3",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.7.4",
    "sass": "^1.96.0",
    "stylelint": "^16.26.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.49.0",
    "unocss": "^66.5.10",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-icons": "^22.5.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^3.1.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "author": "Avivany",
  "license": "MIT"
}
